[Unit]
Description=FRP Launcher Service
After=network.target

[Service]
ExecStart=/usr/local/bin/frp-launcher/frp-launcher.sh --start /usr/local/bin/frp-launcher/portMappings.csv --config /usr/local/bin/frp-launcher/config.toml
ExecStop=/usr/local/bin/frp-launcher/frp-launcher.sh -k
Restart=always
User=root

[Install]
WantedBy=multi-user.target